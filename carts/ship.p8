-- ship

--[[
ship={x,y,spd,sprite,flamespr}

spd:speed of ship
sprite: starting sprite
 note - middle frame
flamesprite: engine flames
 note - first frame / 6 frames 
]]


function updateship()
-- move ⬅️⬆️⬇️➡️ + edge detect
	local dx=0
	local dy=0
	
	if btn(⬅️) then dx-=1 end
	if btn(➡️) then dx+=1 end
	if btn(⬆️) then dy-=1 end
	if btn(⬇️) then dy+=1 end
	
	if dx<0 then
		ship.frame=-1
	elseif dx>0 then
		ship.frame=1
	else
		ship.frame=0
	end
	
	ship.x+=dx*ship.spd
	ship.y+=dy*ship.spd
	
	if ship.x<0 then ship.x=0 end
	if ship.x>120 then ship.x=120 end
	if ship.y<0 then ship.y=0 end
	if ship.y>120 then ship.y=120 end
		
	if ship.inv>0 then
			ship.inv-=1
		 ship.sprite=42
	else
			ship.sprite=2
	end
	
-- flame sprite animation
	ship.flamespr+=1
	if ship.flamespr>9 then
		ship.flamespr=4
	end
end


function drawship()
	if ship.inv>0 then
			if sin(t/4)<0 then
				drawobj(ship)
			end
		else
			drawobj(ship)
			drawflames()
		end
end

function drawflames()
	if ship.frame==-1 then
			spr(ship.flamespr,ship.x+1,ship.y+6)
		elseif ship.frame==1 then
			spr(ship.flamespr,ship.x-1,ship.y+6)
		else
			spr(ship.flamespr,ship.x,ship.y+6)
		end
end
