-- bullets

--[[ 
bullet={x,y,spd,sprite,frame}

sprite: starting sprite
frame: current animation frame
	note - bullets have 3 frmes
spd: speed of bullet
]]

function updatebullets()
	if btn(❎) then
		if #bullets<3 and lastshot>15 then
	 	add(bullets,{x=ship.x,y=ship.y-2,spd=2,sprite=16,frame=0,animspd=0.5,incr=1})
	  lastshot=0
	  muzzlesize=5
	  sfx(0)
		end
	end
	if muzzlesize > 0 then
	 muzzlesize-=1.5
	end
	lastshot+=1
end


function drawbullet(b)
	b.frame+=b.animspd*b.incr
	if b.frame>=3 then
		b.frame-=b.animspd
		b.incr = -1
	elseif b.frame<=0 then
		b.incr = 1
	end
	b.y-=b.spd
	drawobj(b)
	if b.y<0 then del(bullets,b) end
end


