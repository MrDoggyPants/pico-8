-- enemies

-- update
function updateenemies()
	for e in all(enemies) do
	 if collision(ship,e) and ship.inv==0 then
	 	sfx(2)
	 	lives-=1
	 	ship.inv=60
	 end
	 for b in all(bullets) do
	  if collision(b,e) then
	  sfx(1)
	  score+=1
	  del(enemies,e)
	  del(bullets,b)
	  add(explosions,{x=e.x,y=e.y,sprite=57,frame=0,animspd=0.5})
	  end
	 end
	end
end

-- draw
function drawenemy(e)
	e.frame+=e.animspd
	if e.frame>=2 then
		e.frame=0
	end
	e.y+=1
	e.x+=0
	pal(6,e.dark)
	pal(7,e.light)
	drawobj(e)
	pal()
end

function drawexplosion(e)
	e.frame+=e.animspd
	if e.frame>=2 then
		del(explosions,e)
	end
	drawobj(e)
end

function spawnenemies()
	for e in all(enemies) do
		if e.y>127 then
			add(enemies,{x=e.x,y=-8,sprite=e.sprite,frame=e.frame,animspd=e.animspd,dark=e.dark,light=e.light})
			del(enemies,e)
		end
	end
end

