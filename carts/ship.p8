-- space shooter
-- ~mrdoggypants
function _init()
	bg=1
	t=0
	stars={}
 for i=1,25 do
 	add(stars,{x=flr(rnd(128)),y=flr(rnd(128)),spd=rnd(2)+0.5})
 end
 enemies={{x=60,y=-8,sprite=19,frame=0,animspd=0.2,dark=3,light=11}} 
 ship={x=63,y=100,spd=1,sprite=2,frame=0,flamespr=5,inv=0}
 bullets={}
 muzzlesize=0
 lastshot=0
 explosions={}
 pwrups={bomb=false,health=false,gun=true}
 lose=false
 level=1
 score=0
 maxlives=5
 lives=5
end



-->8
-- update
function _update()
	if lose then
		gameover()
	else
		updatebullets()
		updateenemies()
		updateship()
		spawnenemies() 
		if lives<1 then lose=true end
	end
	t+=1
end

-->8
-- draw
function _draw()
	if not lose then
		cls(bg)
		foreach(stars, drawstar)
		foreach(bullets, drawbullet)
		foreach(enemies, drawenemy)
		foreach(explosions, drawexplosion)
		drawship()
		
		if muzzlesize>0 then
			circfill(ship.x+3,ship.y-2,muzzlesize,7)
		end
		
		
		-- hud icons
		print("score: "..score,2,2,7)
		for i=1,maxlives do
			if lives+1>i then
				spr(10,127-i*9,1)
			else 
				spr(11,127-i*9,1)
			end
		end
		if pwrups.bomb==true then
			spr(12,127-9,9)
		else 
			spr(13,127-9,9)
		end
	else
		gameover()
		foreach(stars, drawstar)
	end
end


function drawobj(o)
	spr(o.sprite+o.frame,o.x,o.y)
end

