-- stars
function drawstar(s)
-- colour=speed: fake distance
	local colour=6
	if s.spd<0.5 then
		circfill(s.x,s.y,2,2)
	elseif s.spd<1 then
	 pset(s.x,s.y,5)
	elseif s.spd<2 then
		pset(s.x,s.y,6)
	else
		line(s.x,s.y,s.x,s.y+1,7)
	end

-- move the stars
	s.y+=s.spd

-- remove stars if off screen
	if s.y>128 then
		del(stars, s)
		add(stars,{x=flr(rnd(128)),y=0,spd=rnd(2)+0.49})
	end
end
