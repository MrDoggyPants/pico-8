-- collisions
function collision(a,b)
	local a_left=a.x
	local a_top=a.y
	local a_right=a.x+7
	local a_bot=a.y+7
	
	local b_left=b.x
	local b_top=b.y
	local b_right=b.x+7
	local b_bot=b.y+7
	
	if a_top > b_bot then return false end
	if b_top > a_bot then return false end
	if a_left > b_right then return false end
	if b_left > a_right then return false end
	
	return true
end
