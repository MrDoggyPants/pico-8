-- gameover
function gameover()
 cls(bg)
 print("\^w\^tnot cool",36,48,7)
 print("press ❎ to try again",hcenter("press ❎ to try again"),64,7)
 if btnp(❎) then 
 	lose=false
 	_init()
 end
end

-- text
function hcenter(string)
  return 64-#string*2
end
